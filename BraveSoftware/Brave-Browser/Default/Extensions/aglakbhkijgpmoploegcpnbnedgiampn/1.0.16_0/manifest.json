      "128": "icons/icon-128.png",
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Prodeus",
   "oauth2": {
      "client_id": "432205627649-t7209tb258r3o4gl9gl944utlsn60aph.apps.googleusercontent.com",
      "scopes": [ "profile", "email", "openid" ]
   },
   "permissions": [ "https://www.youtube.com/", "https://api-js.mixpanel.com/*", "https://prodeus-api.herokuapp.com/*", "*://mandrillapp.com/*", "identity", "storage", "activeTab", "tabs" ],
   "short_name": "Prodeus",
